{
  "name": "tsunami-alert-gcp",
  "version": "0.1.0",
  "author": "Phil Gresham",
  "bugs": {
    "email": "phil@gresham.dev",
    "url": "https://github.com/philgresh/tsunami-alert-gcp/issues"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "dot-env": "^0.0.1",
    "firebase": "^9.9.3",
    "firebaseui": "^6.0.1",
    "libphonenumber-js": "^1.10.13",
    "lodash": "^4.17.21",
    "mui-tel-input": "^2.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-firebase-hooks": "^5.0.3",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "export BROWSER='firefox' && react-app-rewired start",
    "build": "mkdir -p build && react-app-rewired build",
    "deploy": "npm run build && cd ./functions && npm run build",
    "postdeploy": "./scripts/deploy.sh $1",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "emulators:start": "export env='LOCAL' && firebase emulators:start --import=\"./local_data\"",
    "emulators:start:export": "export env='LOCAL' && firebase emulators:start --import=\"./local_data\" --export-on-exit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.4",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.38",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "customize-cra": "^1.0.0",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1",
    "storybook-dark-mode": "^1.1.0",
    "webpack": "^5.74.0"
  }
}
